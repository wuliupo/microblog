<% if (success) { %>
<div class="alert alert-success msg">
	<button type="button" class="close" data-dismiss="alert">&times;</button>
	<%= success %>
</div>
<% } %>
<% if (error) { %>
<div class="alert alert-error msg">
	<button type="button" class="close" data-dismiss="alert">&times;</button>
	<%= error %>
</div>
<% } %>